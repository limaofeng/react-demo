{
  "name": "react-demo",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://react-demo.jfantasy.org",
  "repository": {
    "type": "git",
    "url": "git@github.com:limaofeng/react-demo.git"
  },
  "bugs": {
    "url": "https://github.com/limaofeng/react-demo/issues"
  },
  "keywords": [
    "apollo",
    "graphql",
    "react",
    "redux",
    "webpack",
    "router"
  ],
  "author": "limapfeng",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "dependencies": {
    "add-graphql-subscriptions": "^1.0.1",
    "antd": "^2.13.2",
    "apollo-client": "^1.9.3",
    "graphql": "^0.11.3",
    "graphql-tag": "^2.4.2",
    "graphql-tools": "^2.5.0",
    "history": "^4.7.2",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "logrocket": "^0.5.4",
    "logrocket-react": "^0.0.4",
    "nprogress": "^0.2.0",
    "object-assign": "4.1.1",
    "prettier": "^1.7.4",
    "promise": "^8.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-apollo": "^1.4.16",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "subscriptions-transport-ws": "^0.9.0"
  },
  "devDependencies": {
    "apollo-cache-inmemory": "^0.1.0-0",
    "apollo-link": "^0.8.0",
    "apollo-logger": "^0.1.0",
    "apollo-utilities": "^0.1.1-0",
    "autoprefixer": "7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-import": "^1.5.0",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "^6.26.0",
    "bundle-loader": "^0.5.5",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^1.1.3",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-graphql": "^1.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "^3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "^20.0.4",
    "jest-transform-graphql": "^2.1.0",
    "jquery": "^3.2.1",
    "jsdom": "^11.3.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.6",
    "react-dev-utils": "^4.0.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^16.0.0",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "style-loader": "^0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.1",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-server": "^2.7.1",
    "webpack-manifest-plugin": "^1.2.1",
    "whatwg-fetch": "^2.0.3",
    "why-did-you-update": "^0.0.8"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "raf/polyfill",
      "<rootDir>/src/testHelpers/env.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "style": true
        }
      ],
      "transform-decorators-legacy",
      [
        "babel-plugin-inline-import",
        {
          "extensions": [
            ".graphqls"
          ]
        }
      ]
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true
  },
  "lint-staged": {
    "src/**/*.{js,jsx,graphql}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "jest": true,
      "jquery": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "webpack/webpack.config.dev.js"
        }
      }
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "indent": 0,
      "space-before-function-paren": 0,
      "prettier/prettier": "error",
      "global-require": "off",
      "comma-dangle": 0,
      "no-use-before-define": 0,
      "no-param-reassign": "warn",
      "react/prefer-stateless-function": "warn",
      "no-console": 1,
      "max-len": [
        2,
        120,
        4
      ],
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "consistent-return": 0,
      "no-confusing-arrow": 0,
      "no-shadow": 0,
      "no-underscore-dangle": 0,
      "no-debugger": 1,
      "no-continue": "warn",
      "no-plusplus": [
        "error",
        {
          "allowForLoopAfterthoughts": true
        }
      ],
      "semi": 2,
      "import/no-extraneous-dependencies": 0,
      "import/prefer-default-export": 0,
      "import/no-duplicates": 0,
      "react/no-multi-comp": 0,
      "react/no-array-index-key": [
        1
      ],
      "react/forbid-prop-types": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  }
}
